<?xml version="1.0"?>
<object
    name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="leam.simmap"
    purge="False">

    <configlet
        title="SimMap"
        action_id="simmap"
        appId="leam.simmap"
        category="Products"
        condition_expr=""
